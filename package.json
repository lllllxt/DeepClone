  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lllllxt/DeepClone/issues"
  },
  "homepage": "https://github.com/lllllxt/DeepClone#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "jest": "^25.4.0",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "files": [
    "src/",
    "clone.js",
    "clone.min.js"
  ]
